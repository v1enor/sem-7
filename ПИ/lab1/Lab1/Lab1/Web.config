<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1"/>
    <httpRuntime targetFramework="4.8.1"/>
  </system.web>
  <system.webServer>
    <handlers>
      <add name ="smwHandler" path ="/handler" verb ="GET" type="lab01.IISHandler1"/>
      <add name ="PIA_GET" path ="/pia_get" verb ="GET" type="lab01.PIA_GET"/>
      <add name ="PIA_POST" path ="/pia_post" verb ="POST" type="lab01.PIA_POST"/>
      <add name ="PIA_PUT" path ="/pia_put" verb ="PUT" type="lab01.PIA_PUT"/>
      <add name ="PIA_TASK4" path ="/task4" verb ="POST" type="lab01.PIA_TASK4"/>
      <add name="PIA_TASK5" path="/task5" verb="GET,POST" type="lab01.PIA_TASK5" />
      <add name="PIA_TASK6" path="/task6" verb="GET,POST" type="lab01.PIA_TASK6" />
      <add name="websockets" path="*.websocket" verb="GET" type="lab01.PIA_TASK7" />
    </handlers>
  </system.webServer>
</configuration>
